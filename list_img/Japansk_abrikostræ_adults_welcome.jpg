metræ, inklusive blomsterkrukke og sokkel, der er over 34 cm højt, 17 cm bredt og 9 cm dybt\u003c/li\u003e\u003c/ul\u003e","canAddToWishlist":true,"maxOrderQuantity":5,"skuSelectorValue":null,"showStoreInventory":true,"showReviews":true,"bisNotificationState":"HIDDEN","featuredFlags":[],"safetyWarning":{"type":"id","generated":true,"id":"$ProductVariant:6506049.attributes.safetyWarning","typename":"ProductSafetyWarning"}},"ProductImage:bltd6f8360aaf62c494":{"id":"bltd6f8360aaf62c494","__typename":"ProductImage","baseImgUrl":"https://www.lego.com/cdn/cs/set/assets/bltd6f8360aaf62c494/10369_Prod.png","sizes":{"type":"id","generated":true,"id":"$ProductImage:bltd6f8360aaf62c494.sizes","typename":"ProductImageSizes"}},"ProductImage:bltfb7d7a6f01832c91":{"id":"bltfb7d7a6f01832c91","__typename":"ProductImage","baseImgUrl":"https://www.lego.com/cdn/cs/set/assets/bltfb7d7a6f01832c91/10369_boxprod_v39_sha.jpg","sizes":{"type":"id","generated":true,"id":"$ProductImage:bltfb7d7a6f01832c91.sizes","typename":"ProductImageSizes"}},"ProductVideo:bltb312189ad0129b54":{"id":"bltb312189ad0129b54","__typename":"ProductVideo","video":{"type":"id","generated":true,"id":"$ProductVideo:bltb312189ad0129b54.video","typename":"ProductVideoFormat"}},"ProductImage:blt187866f0c6749ae9":{"id":"blt187866f0c6749ae9","__typename":"ProductImage","baseImgUrl":"https://www.lego.com/cdn/cs/set/assets/blt187866f0c6749ae9/10369_WEB_SEC01_NOBG.png","sizes":{"type":"id","generated":true,"id":"$ProductImage:blt187866f0c6749ae9.sizes","typename":"ProductImageSizes"}},"ProductImage:blt2cf1b299137a90cb":{"id":"blt2cf1b299137a90cb","__typename":"ProductImage","baseImgUrl":"https://www.lego.com/cdn/cs/set/assets/blt2cf1b299137a90cb/10369_WEB_SEC02_NOBG.png","sizes":{"type":"id","generated":true,"id":"$ProductImage:blt2cf1b299137a90cb.sizes","typename":"ProductImageSizes"}},"ProductImage:blt5c12fcb483fe18e3":{"id":"blt5c12fcb483fe18e3","__typename":"ProductImage","baseImgUrl":"https://www.lego.com/cdn/cs/set/asse